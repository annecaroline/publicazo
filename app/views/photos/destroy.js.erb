$('#photos').html("<%= j render 'photos_list' %>")
<% if @service.photos.blank? %>
  $('#photo_check').hide();  
  $('#unpublish_button').click();
  $('#unpublish_button').attr('disabled', false);
  $('#photo_required').show();
<% end %>

<% if !@service.active && !@service.price.blank? && !@service.listing_name.blank? && !@service.photos.blank? && !@service.address.blank?  && !@service.is_independent.nil? %>
    $('#publish_button').attr('disabled', false);
    $('#unpublish_button').attr('disabled', true);    
<% else %>
    $('#publish_button').attr('disabled', true);
    $('#unpublish_button').attr('disabled', false);
    $('#photo_required').show();
<% end %>
